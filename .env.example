# Focal-Point Environment Configuration
# Copy this file to .env and fill in your values

# Required: Session secret for secure session management
# Generate a secure random string (e.g., using: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
SESSION_SECRET=your-secure-session-secret-here

# Optional: Server port (defaults to 3000)
PORT=3000

# Optional: Environment mode (development/production)
NODE_ENV=development

# Optional: Trust proxy setting (set to 1 if behind reverse proxy)
TRUST_PROXY=0